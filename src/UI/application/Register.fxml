<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.PasswordField?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="400.0" prefWidth="300.0" xmlns="http://javafx.com/javafx/15.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="application.RegisterController">
   <children>
      <Button layoutX="113.0" layoutY="327.0" mnemonicParsing="false" onAction="#register" text="Sign Up">
         <font>
            <Font size="18.0" />
         </font>
      </Button>
      <TextField fx:id="txtUsername" layoutX="39.0" layoutY="176.0" promptText="Username" AnchorPane.topAnchor="170.0">
         <font>
            <Font size="18.0" />
         </font>
      </TextField>
      <PasswordField fx:id="txtPassword1" layoutX="39.0" layoutY="231.0" promptText="Password" AnchorPane.topAnchor="220.0">
         <font>
            <Font size="18.0" />
         </font>
      </PasswordField>
      <PasswordField fx:id="txtPassword2" layoutX="40.0" layoutY="286.0" promptText="Repeat Password" AnchorPane.topAnchor="270.0">
         <font>
            <Font size="18.0" />
         </font>
      </PasswordField>
      <Label fx:id="lblStatus" layoutX="15.0" layoutY="27.0" text="Status" textFill="#ab3030" AnchorPane.leftAnchor="15.0" AnchorPane.rightAnchor="15.0">
         <font>
            <Font size="18.0" />
         </font>
      </Label>
      <TextField fx:id="txtName" layoutX="40.0" layoutY="72.0" promptText="Full name" AnchorPane.topAnchor="70.0">
         <font>
            <Font size="18.0" />
         </font>
      </TextField>
      <TextField fx:id="txtEmail" layoutX="39.0" layoutY="120.0" promptText="Email" AnchorPane.topAnchor="120.0">
         <font>
            <Font size="18.0" />
         </font>
      </TextField>
   </children>
</AnchorPane>
